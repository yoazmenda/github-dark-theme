{
    "manifest_version": 2,
    "name": "GitHub Dark Theme",
    "description": "A Dark theme for all of GitHub based on Atom One Dark.",
    "version": "1.0.41",
    "browser_action": {
        "default_icon": "icons/icon.png",
        "default_popup": "app/popup.html"
    },
    "background": {
        "scripts": [
            "app/vendor.js",
            "app/background.js"
        ]
    },
    "permissions": [
        "storage",
        "*://*/*"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}